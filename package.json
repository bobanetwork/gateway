    "collectCoverageFrom": [
      "src/components/**/*.tsx",
      "src/containers/**/*.tsx",
      "src/layout/**/*.tsx",
      "src/util/**/*.ts",
      "src/services/**/*.ts",
      "!src/actions/**/*.ts",
      "!src/api/**/*.ts",
      "!src/selectors/**/*.ts",
      "!src/reducers/**/*.ts",
      "!src/store/**/*.tsx",
